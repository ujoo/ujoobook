<html>
<head>
<%- include('./headerHtml.ejs') %>
</head>
<style>
#etnewsDate { width:80px; }
</style><body>
<%- include('./header.ejs') %>

<div class="container">
    <div class="row">
        <div class="col-lg-12">

            <div class="bs-component">
                <table class="table table-striped table-hover ">
                <colgroup><col width="20%"><col width="*"></colgroup>
                <tbody>
                    <tr>
                    <td colspan="2">
                        <input type="text" name="etnewsDate" id="etnewsDate" value="<%=etnewsDate%>">일자 <%=title%>
                        <span class="btn btn-info" id="btnEtnews">보기</span>
                    </td>
                    </tr>
                    <% if (htmlurl) { %>
                    <tr>
                    <td>
                    <a href="<%=htmlurl%>" target="_blank">신문보기</a>
                    </td>
                    </tr>
                    <%} else { %>
                    <tr>
                    <td>등록된 신문이 없습니다.</td>
                    </tr>
                    <% }%>
                </tbody>
                </table> 
            </div>
        </div>

    </div>

    <%- include('./footer.ejs') %>


</div>

    <script type="text/javascript">
    $(document).ready(function() {
        $(document).on("click", "#btnEtnews", function() {
            location.href = "/hani?etnewsDate=" + $("#etnewsDate").val();
        });
       
    });
    </script>
</body>
</html>
